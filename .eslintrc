{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier", "eslint-plugin-prettier"],
  "ignorePatterns": ["src/dist/*", "src/reportWebVitals.js", "src/setupTests.js"],
  "rules": {
    "prettier/prettier": 1
  }
}
